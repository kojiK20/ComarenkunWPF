            </ColorAnimationUsingKeyFrames>
        </Storyboard>



    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <!--<BeginStoryboard Storyboard="{StaticResource MemberButtonMouseOver}"/>
            <BeginStoryboard Storyboard="{StaticResource MatchingButtonMouseOver}"/>-->
            <BeginStoryboard x:Name="InitialBall_BeginStoryboard" Storyboard="{StaticResource InitialBall}"/>
            <BeginStoryboard x:Name="MatchingButtonMouseLeave_BeginStoryboard" Storyboard="{StaticResource MatchingButtonMouseLeave}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseEnter" SourceName="comarenkunButton">
            <RemoveStoryboard BeginStoryboardName="ComarenkunMouseLeave_BeginStoryboard1"/>
            <BeginStoryboard x:Name="ComarenkunMouseOver_BeginStoryboard1" Storyboard="{StaticResource ComarenkunMouseOver}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave" SourceName="comarenkunButton">

            <BeginStoryboard x:Name="ComarenkunMouseLeave_BeginStoryboard1" Storyboard="{StaticResource ComarenkunMouseLeave}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="comarenkunButton">

            <BeginStoryboard x:Name="ComarenkunClicked_BeginStoryboard1" Storyboard="{StaticResource ComarenkunClicked}"/>
        </EventTrigger>
        <!--<EventTrigger RoutedEvent="UIElement.MouseLeftButtonDown">
            <BeginStoryboard x:Name="Clickeffect1_BeginStoryboard" Storyboard="{StaticResource Clickeffect1}"/>
        </EventTrigger>-->
    </Window.Triggers>

